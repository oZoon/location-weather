{
  "presets": [
    "@babel/preset-env",
    "@babel/preset-typescript",
    "@babel/preset-react"
  ],
  "plugins": [
    "babel-plugin-styled-components",
    ["@babel/plugin-proposal-decorators", { "version": "legacy" }],
    [
      "module-resolver",
      {
        "root": ["./src"],
        "alias": {
          "@": "./src"
        }
      }
    ]
  ],
  "assumptions": {
    "setPublicClassFields": false
  }
}
